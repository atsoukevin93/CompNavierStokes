    "import numpy as np\n",
    "\n",
    "\n",
    "def centered_mean(x,y):\n",
    "    return (x+y)/2.\n",
    "\n",
    "\n",
    "def shiftg(x):\n",
    "    b=len(x)\n",
    "    return np.concatenate([x[np.arange(1, b, 1)], np.array([x[0]])])\n",
    "\n",
    "\n",
    "def shiftd(x):\n",
    "    b=len(x)\n",
    "    return x[np.arange(-1, b-1, 1)]\n"
